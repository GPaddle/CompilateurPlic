//Declaration de 1 tableau & 2 entiers, assignation des 2 entiers & du tableau (avec indice variable et valeur variable), ecriture du tableau avec indice variable
programme x {
    tableau [ 3 ] tab ;	//0 -> 	8
    entier x ;			//12
    entier y ;			//14

	x := 2 ;
	y := 3 ;

    tab [ 0 ] := 1 ;
    tab [ 1 ] := 2 ;
    tab [ x ] := y ;	//8 := 14

    ecrire tab [ x ] ;
    //Attendu :
    //3
}