programme whileNested {
    entier x ;
    tableau [ 1 ] tab ;
    tab [ 0 ] := 1 ;
    x := 0 ;
    tantque ( x < 2 ) repeter {
        ecrire x ;
        tantque ( tab [ 0 ] < 4 ) repeter {
            ecrire tab [ 0 ] ;
            tab [ 0 ] := tab [ 0 ] + 2 ;
        }
        x := x + 1 ;
    }
}