//Declaration de 3 entiers, assignation des entiers, ecriture de differentes comparaisons
programme x {

	entier x ;
	entier y ;
	entier z ;
	
	x := 1 ; 
	y := 2 ;
	z := 2 ;
	ecrire ( x < y ) ;
	ecrire ( x <= y ) ;
	ecrire ( x > y ) ;
	ecrire ( x >= y ) ;
	ecrire ( y = z ) ;
	ecrire ( x # z ) ;
		
