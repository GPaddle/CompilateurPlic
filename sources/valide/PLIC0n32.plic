//Declaration de 2 entiers & 1 tableau, assignation des 2 entiers & du tableau (avec indice variable), ecriture du tableau avec indice variable
programme PLICcoucou {
	entier b ;
	entier c ;
	tableau [ 5 ] a ;
	
	b := 2 ;
	c := 3 ;

	a [ 0 ] := 1 ;
	a [ 1 ] := 2 ;
	
	a [ b ] := 3 ;
	
	a [ 3 ] := 4 ;
	a [ 4 ] := 5 ;
	
	ecrire a [ b ] ;
	ecrire a [ c ] ;
}