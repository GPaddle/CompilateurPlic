//Declaration de 2 entiers 1 tableau, ecriture d'entier, assignation de 1 variable & 1 tableau avec indice defini par variable, ecriture du tableau
programme PLICcoucou {
	entier b ;
	entier c ;
	tableau [ 5 ] a ;
	
	ecrire 5 ;
	
	c := 2 ;
	
	a [ c ] := 7 ;
	ecrire a [ c ] ;
	//Attendu :
	//5
	//7
}